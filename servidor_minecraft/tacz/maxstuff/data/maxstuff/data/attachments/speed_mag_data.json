{
    // 配件重量，单位 kg。影响持枪者的移速
    "weight": 0.2,
    // 对瞄准时间的影响，加数，单位为秒，可为负数
    "ads_addend": -0.02,
    // 扩容弹夹（匣）等级
    "extended_mag_level": 2,
    // 对枪械后坐力的影响。
    "recoil_modifier": {
      // 竖直方向偏转削减的大小，范围-1~0
      "pitch": -0.15,
      // 水平方向偏转削减程度的大小，范围-1~0
      "yaw": -0.2
    }
  }