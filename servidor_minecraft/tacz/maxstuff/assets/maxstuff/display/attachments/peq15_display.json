{
  "slot": "maxstuff:attachment/slot/tac_laser",
  // 配件的默认模型，不可为空
  "model": "maxstuff:attachment/tac_laser_geo",
  // 配件默认模型使用的材质，不可为空
  "texture": "maxstuff:attachment/uv/tac_laser",
  // 只有 瞄具 配件需要设置这个属性，表示瞄具的放大倍率。
  // 玩家可以在数组设置的几个倍率之间切换
  "lod": {
    "model": "maxstuff:attachment/lod/tac_laser_lod",
    "texture": "maxstuff:attachment/lod/tac_laser_lod"
  },
  "sounds": {
    "install": "tacz:attachments/scope_general_c",
    "uninstall": "tacz:attachments/scope_general_c"
  },
  "laser": {
    // 默认的镭射颜色 default laser color, 0x = #
    "default_color": "#e2fbf8",
    // 是否允许在改装界面自定义该镭射的颜色 can be customized
    "can_edit": true,
    // 激光的长度（因为是渐变，末端可能实际完全看不见）laser length
    "length": 5.4
  }
}