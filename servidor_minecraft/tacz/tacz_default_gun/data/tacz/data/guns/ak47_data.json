      // 加算，多连发模式的开火数值调整，如无调整可不写
      // "damage": 0,
      // "rpm": 0,
      // "speed": 0,
      // "knockback": 0,
      // "armor_ignore": 0,
      // "head_shot_multiplier": 0,
      // "aim_inaccuracy": 0,
      // "other_inaccuracy": 0
    // }
  },
  */
  // 多连续发数据，仅多连发会调用
  "burst_data": {
    // 是否连续射击
    "continuous_shoot": true,
    // 连发数
    "count": 3,
    // 组内连发的射速
    "bpm": 900,
    // 每组连发之间的时间间隔（上一组结束时间到下一组开始的时间间隔），单位秒
    "min_interval": 0.3
  },
  "crawl_recoil_multiplier": 0.5,
  // 后坐力
  "recoil": {
    "pitch": [
      {"time": 0, "value": [0.66, 0.66]},
      {"time": 0.1, "value": [0.66, 0.66]},
      {"time": 0.45, "value": [-0.175, -0.175]},
      {"time": 0.6, "value": [0, 0]}
    ],
    "yaw": [
      {"time": 0, "value": [-0.23, 0.18]},
      {"time": 0.35, "value": [0, 0]}
    ]
  },
  // 受伤晃动幅度修改，乘数，1 为原版晃动程度，0 为完全不晃动，默认为 0.05
  "hurt_bob_tweak_multiplier": 0.05,
  // 不准确度
  "inaccuracy": {
    // 站立射击散布
    "stand": 4.5,
    // 移动射击散布
    "move": 5,
    // 潜行射击散布
    "sneak": 2.5,
    // 趴下射击散布
    "lie": 1.5,
    // 瞄准射击时散布
    "aim": 0.15
  },
  // 近战相关
  "melee": {
    // 枪械距离参数，用来延升近战攻击范围
    // 会与刺刀等配件的距离做加和
    "distance": 1,
    // 使用时的冷却时间
    // 会与刺刀等配件的冷却时间做加和
    "cooldown": 0.7,
    // 默认近战数据，会被配件的数据替换掉
    "default": {
      // 动画类型：没有枪托的为 melee_push，有枪托的为 melee_stock
      "animation_type": "melee_push",
      // 刺刀距离参数，枪械里还有个刺刀攻击距离参数，两者做加和
      "distance": 1,
      // 刺刀范围角度
      "range_angle": 40,
      // 刺刀冷却时间，枪械里还有个刺刀冷却时间参数，两者做加和
      "cooldown": 0,
      // 伤害值
      "damage": 1,
      // 击退效果
      "knockback": 0.5,
      // 前摇时长，单位：秒
      "prep": 0.1
    }
  },
  // 开放的配件槽。未指定的槽位默认关闭。全部配件槽类型有:
  // scope, stock, muzzle, grip, laser, extended_mag
  "allow_attachment_types": [
    "scope",
    "stock",
    "muzzle",
    "extended_mag"
  ]
  // 配件槽的默认配件，当未安装配件时使用
  // 默认配件的data将不会被应用，仅仅有外观效果
  // 用于默认瞄具等特殊场合
 //  "builtin_attachments": {
 //    "scope": "tacz:scope_standard_8x"
